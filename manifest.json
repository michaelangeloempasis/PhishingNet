{
  "manifest_version": 3,
  "name": "PhishingNet",
  "version": "0.2",
  "permissions": [
    "storage",
    "notifications",
    "scripting",
    "activeTab",
    "tabs",
    "clipboardRead"
  ],
  "host_permissions": ["<all_urls>"],
  "background": { "service_worker": "service_worker.js" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"],
      "run_at": "document_start"
    }
  ],
  "action": { "default_popup": "popup.html" },
  "icons": {
    "16": "icons/Phishing logo.jpg",
    "48": "icons/Phishing logo.jpg",
    "128": "icons/Phishing logo.jpg"
  },
  "web_accessible_resources": [
    {
      "resources": ["sounds/*","icons/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
